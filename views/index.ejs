            animation: fadeInRight 1.5s both;
        }

        @-webkit-keyframes fadeInRight{
            0%{
                opacity: 0;
                -webkit-transform: translateX(50px);
            }
            100%{
                opacity: 1;
                -webkit-transform: translateX(0);
            }
        }
        @-moz-keyframes fadeInRight{
            0%{
                opacity: 0;
                -moz-transform: translateX(50px);
            }
            100%{
                opacity: 1;
