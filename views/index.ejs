    <script>
        const observer = new IntersectionObserver(entries =>{
            entries.forEach(entry => {;
                if(entry.isIntersecting){
                    document.querySelectorAll(".vision")[0].classList.add("fadeInBottom");
                    document.querySelectorAll(".mission")[0].classList.add("fadeInBottom");
                }
            })
        })

        observer.observe(document.querySelector(".row"));
    </script>